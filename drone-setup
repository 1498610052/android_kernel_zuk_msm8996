apt-get update
apt-get install -y build-essential bc curl git zip ftp ccache python zstd jq wget tar gcc-aarch64-linux-gnu gcc-arm-linux-gnueabi

cd $HOME
#Get and install git-lfs
export LFS_VERSION="v2.8.0"
export LFS_NAME="git-lfs-linux-amd64-${LFS_VERSION}.tar.gz"
mkdir lfs
cd lfs
wget https://github.com/git-lfs/git-lfs/releases/download/${LFS_VERSION}/${LFS_NAME}
tar -xzf ${LFS_NAME}
./install.sh

cd $HOME
git clone --depth=1 https://github.com/xzr467706992/AnyKernel2
git clone --depth=1 https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86
