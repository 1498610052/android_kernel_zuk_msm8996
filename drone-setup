apt-get update
apt-get install -y build-essential bc python curl git zip ftp gcc-aarch64-linux-gnu gcc-arm-linux-gnueabi libssl-dev lftp zstd wget libfl-dev

CBL="LiuNian_clang-20200322.tar.zst"

cd $HOME
git clone --depth=1 https://github.com/xzr467706992/AnyKernel2
wget https://dl.akr-developers.com/s/CBL/${CBL}
mkdir cbl
tar -I zstd -xvf ${CBL} -C cbl
